{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/prisma/*": [
        "prisma/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/lib/*": [
        "lib/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "prisma/prismaClient.js",
    "components/specblank.spec.js",
    "jest.config.js"
, "components/forms/signin/Signin.spec.js"  ],
  "exclude": [
    "node_modules"
  ]
}
